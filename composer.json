{
  "name": "mtpal/crud-generator",
  "description": "A crud generator",
  "license": "MIT",
  "keywords": [
    "crud",
    "generator"
  ],
  "type": "library",
  "authors": [
    {
      "name": "",
      "email": ""
    }
  ],
  "require": {
    "php": ">=7.0",
    "illuminate/support": "~5.7.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Mtpal\\CrudGenerator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mtpal\\CrudGenerator\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Mtpal\\CrudGenerator\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
